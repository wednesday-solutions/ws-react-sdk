{
  "name": "ws-react-sdk",
  "version": "0.0.1",
  "sideEffects": false,
  "description": "Library of common components, containers, and utils used across the Wednesday Ecosystem",
  "main": "lib/es/index.js",
  "browser": "lib/es/index.js",
  "files": [
    "lib",
    "lib/es",
    "es/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/wednesday-solutions/ws-react-sdk.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Wednesday Solutions",
  "homepage": "https://github.com/wednesday-solutions/ws-react-sdk",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn install-peers && ./rollup-master.sh",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "package-sdk": "yarn rollup:prod && git add lib/ && git commit -m 'created lib'",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:staged": "jest --findRelatedTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "rollup:dev:services": "npx rollup --config=rollup/rollup.services.config.js --environment ENV_NAME:development,BUILD:production",
    "rollup:dev:themes": "npx rollup --config=rollup/rollup.themes.config.js --environment ENV_NAME:development,BUILD:production",
    "rollup:dev:components": "npx rollup --config=rollup/rollup.components.config.js --environment ENV_NAME:development,BUILD:production",
    "rollup:dev:utils": "npx rollup --config=rollup/rollup.utils.config.js --environment ENV_NAME:development,BUILD:production",
    "rollup:watch:services": "watch 'yarn rollup:dev:services --environment BUILD:development' ./src/services",
    "rollup:watch:themes": "watch 'yarn rollup:dev:themes --environment BUILD:development' ./src/themes",
    "rollup:watch:components": "watch 'yarn rollup:dev:components --environment BUILD:development' ./src/components",
    "rollup:watch:utils": "watch 'yarn rollup:dev:utils --environment BUILD:development' ./src/utils",
    "rollup:watch": "yarn rollup:watch:services | yarn rollup:watch:themes | yarn rollup:watch:utils | yarn rollup:watch:components",
    "rollup:dev": "./rollup-develop.sh",
    "rollup:prod": "./rollup-master.sh",
    "webpack": "cross-env NODE_ENV=development  && npx webpack -c ./webpack.config.js --mode=development",
    "webpack:prod": "cross-env NODE_ENV=production && npx webpack -c ./webpack.config.js --mode=production",
    "prepublishOnly": "yarn webpack:prod",
    "dev": "rm -rf lib && yarn rollup:watch"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix .",
      "git add --force",
      "jest --findRelatedTests $STAGED_FILES"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "pre-push": "npm test",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@rollup/plugin-image": "^2.0.6",
    "add": "^2.0.6",
    "amazon-cognito-identity-js": "4.5.7",
    "antd": "4.10.2",
    "axios": "^0.21.1",
    "cross-env": "5.2.0",
    "immer": "3.0.0",
    "less": "^4.1.0",
    "libphonenumber-js": "^1.9.8",
    "lodash": "4.17.20",
    "moment": "^2.29.1",
    "react-easy-crop": "^3.3.1",
    "react-intl": "^4.5.1",
    "reselect": "^4.0.0",
    "style-loader": "^2.0.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.0",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@testing-library/react": "^11.2.3",
    "axios-mock-adapter": "latest",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "^6.24.1",
    "connected-react-router": "^6.8.0",
    "coveralls": "3.0.3",
    "dotenv": "^8.2.0",
    "es-dev-server-import-json": "^2.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "3.0.1",
    "husky": "^4.3.8",
    "image-webpack-loader": "4.6.0",
    "imports-loader": "0.8.0",
    "install-peers-cli": "^2.2.0",
    "jest-cli": "24.7.1",
    "jest-dom": "3.1.3",
    "lint-staged": "8.1.5",
    "postcss": "^8.2.4",
    "pre-commit": "^1.2.2",
    "prettier": "1.17.0",
    "prettier-config-standard": "1.0.1",
    "prettier-standard": "15.0.1",
    "rimraf": "2.6.3",
    "rollup": "^2.36.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-export-default": "^1.4.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-url": "^3.0.1",
    "svg-url-loader": "2.3.2",
    "terser-webpack-plugin": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "1.1.2",
    "watch": "^1.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2"
  },
  "peerDependencies": {
    "antd": "4.10.2",
    "react": "16.8",
    "react-dom": "16.8",
    "react-redux": "7.0.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.1.0",
    "redux-saga": "1.1.3",
    "reduxsauce": "^1.1.0",
    "styled-components": "5.2.1"
  }
}
